<!DOCTYPE html>
<html>

  <head>
    <meta charset="utf-8">
    <title>Admin</title>
    <%- include layout/materialize %>
  </head>

  <body>
    <%- include layout/innerheader_admin %>
      <div class="container">

        <br><br><br>
        <table class="responsive-table">
          <tr>
            <th>No</th>
            <th>Customer</th>
            <th>Username</th>
            <th>Options</th>
            <th>Actions</th>
          </tr>

          <% x = 1 %>
            <%dataUser.forEach(d => {%>
              <tr>
                <td>
                  <%= x++ %>
                </td>
                <td>
                  <%=d.name%>
                </td>
                <td>
                  <%=d.username%>
                </td>
                <td><a href="/admin/usercar/<%=d.id%>" class="waves-effect waves-light btn #37474f blue-grey darken-3">Detail Pembelian</a></td>
                <td><a href="/admin/user/delete/<%=d.id%>" class="waves-effect waves-light btn #37474f blue-grey darken-3">Delete</a></td>
              </tr>
              <%})%>
        </table>
      </div>
  </body>

</html>

</html>
